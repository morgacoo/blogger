<h1>All Tags</h1>

<ul id="tags">
  <% @tags.each do |tag| %>
    <li>
      <%= link_to tag.title, article_path(tag) %>
    </li>
  <% end %>
</ul>

<%= link_to "Create a New Article", new_article_path, class: 'new_article' %>
<%= link_to "delete",  article_path(@tag), method: :delete, data: {confirm: "Really want to delete the tag?"} %>
